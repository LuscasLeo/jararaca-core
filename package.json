{
  "name": "@jararaca/core",
  "version": "0.1.0",
  "description": "A collection of React hooks and utilities",
  "type": "module",
  "main": "dist/jararaca-core.cjs.js",
  "module": "dist/jararaca-core.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/jararaca-core.es.js",
      "require": "./dist/jararaca-core.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --p ./tsconfig-build.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=5.0.0",
    "axios": ">=1.0.0",
    "immutable": ">=5.0.0",
    "notistack": ">=3.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "usehooks-ts": ">=3.0.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.48.5",
    "@eslint/js": "^9.25.1",
    "@tanstack/react-query": "^5.74.4",
    "@types/node": "^22.15.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "axios": "^1.9.0",
    "eslint": "^9.22.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "immutable": "^5.1.1",
    "lint-staged": "^15.5.1",
    "notistack": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "usehooks-ts": "^3.1.1",
    "vite": "^6.3.1",
    "vite-plugin-dts": "^4.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "keywords": [
    "react",
    "hooks",
    "typescript",
    "frontend"
  ],
  "author": "",
  "license": "MIT"
}